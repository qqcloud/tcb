{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["./app/libs"],
		"exclude": ["./node_modules/"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	},
	"opts": {
		"template": "templates/default", 
		"encoding": "utf8", 
		"destination": "./jsdoc",
		"recurse": true
	}
}